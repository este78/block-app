  "source": "pragma solidity ^0.5.0;\n\ncontract IfElse {\n    function foo(uint x) public pure returns (uint) {\n        if (x < 10) {\n            return 0;\n        } \n        if (x < 20){\n            return 1;\n        }\n        else {\n            return 2;\n        }\n    }\n}",
